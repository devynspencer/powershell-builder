<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add
        key="github"
        value="https://nuget.pkg.github.com/%GITHUB_USERNAME%/index.json"
        />
    <add
        key="nuget.org"
        value="https://api.nuget.org/v3/index.json"
        />
    <add
        key="Staging"
        value="temp\build"
        />
  </packageSources>
  <packageSourceCredentials>
    <github>
      <add
          key="Username"
          value="%GITHUB_USERNAME%"
          />
      <add
          key="ClearTextPassword"
          value="%GITHUB_PACKAGES_TOKEN%"
          />
    </github>
  </packageSourceCredentials>
</configuration>
